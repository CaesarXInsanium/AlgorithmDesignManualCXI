# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  libadm_config = debug

else ifeq ($(config),release)
  libadm_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := libadm

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

libadm:
ifneq (,$(libadm_config))
	@echo "==== Building libadm ($(libadm_config)) ===="
	@${MAKE} --no-print-directory -C . -f libadm.make config=$(libadm_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f libadm.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   libadm"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"